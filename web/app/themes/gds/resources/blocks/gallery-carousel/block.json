{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gds/gallery-carousel",
  "title": "Gallery Carousel",
  "description": "Display a gallery with a carousel",
  "category": "media",
  "icon": "images-alt",
  "attributes": {
    "media": {
      "type": "array",
      "default": []
    },
    "ariaLabel": {
      "type": "string"
    }
  },
  "supports": {
    "align": ["full", "wide"],
    "customClassName": true,
    "html": false,
    "color": {}
  },
  "example": {
    "attributes": {
      "columnCount": 2,
      "media": [
        {"url": "https://cldup.com/Fz-ASbo2s3.jpg", "type": "image"},
        {"url": "https://cldup.com/Fz-ASbo2s3.jpg", "type": "image"}
      ]
    },
    "viewportWidth": 400
  },
  "editorScript": "file:./index.js",
  "style": "file:/style.css"
}
