{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gds/carousel",
  "title": "Carousel",
  "description": "Display a carousel",
  "category": "layout",
  "icon": "format-gallery",
  "attributes": {
    "columnCount": {
      "type": "integer",
      "default": 1
    },
    "ariaLabel": {
      "type": "string"
    }
  },
  "supports": {
    "align": ["full", "wide"],
    "customClassName": true,
    "html": false,
    "color": {},
    "__experimentalExposeControlsToChildren": true,
    "__experimentalCaptureToolbars": true
  },
  "example": {
    "innerBlocks": [
      {
        "name": "gds/carousel-item",
        "innerBlocks": [
          { "name": "core/image", "attributes": { "url": "https://cldup.com/Fz-ASbo2s3.jpg" } }
        ]
      },
      {
        "name": "gds/carousel-item",
        "innerBlocks": [
          { "name": "core/image", "attributes": { "url": "https://cldup.com/Fz-ASbo2s3.jpg" } }
        ]
      },
      {
        "name": "gds/carousel-item",
        "innerBlocks": [
          { "name": "core/image", "attributes": { "url": "https://cldup.com/Fz-ASbo2s3.jpg" } }
        ]
      }
    ],
    "attributes": {
      "columnCount": 2
    },
    "viewportWidth": 800
  },
  "editorScript": "file:./index.js",
  "style": "file:/style.css"
}
